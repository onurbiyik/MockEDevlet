{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "dist/**/*.*",
        "dist/jquery.min.js",
        "dist/jquery.min.map",
        "dist/jquery.slim.js",
        "dist/jquery.slim.min.js",
        "dist/jquery.slim.min.map"
      ]
    },
    {
      "library": "bootstrap@5.1.3",
      "destination": "wwwroot/lib/bootstrap",
      "files": [ "dist/**/*.*" ]
    }
  ]
}